{
  "private": true,
  "name": "pretty-good-playground-api",
  "version": "1.0.0",
  "description": "A template for kick starting a Cloudflare Workers project",
  "main": "dist/pretty-good-playground-api.es.js",
  "scripts": {
    "build": "tsc && vite build",
    "test": "vitest --run",
    "format": "prettier --write '**/*.{js,css,json,md}'",
    "lint": "prettier --write '**/*.{js,ts,md}' && eslint src/**",
    "lint-fix": "prettier --write . && eslint src/** --fix",
    "coverage": "nyc --reporter=lcov npm run test"
  },
  "author": "Shane Lucy <shanelucy01@protonmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^3.5.0",
    "@types/jest": "^27.4.1",
    "c8": "^7.11.0",
    "eslint-config-typescript-airbnb-prettier": "^1.1.2",
    "eslint-plugin-jest": "^26.1.4",
    "husky": "^7.0.4",
    "nyc": "^15.1.0",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "vite": "^2.9.5",
    "vitest": "^0.9.3"
  },
  "dependencies": {
    "@peculiar/webcrypto": "^1.3.3",
    "@types/node": "^17.0.24",
    "itty-router": "^2.6.1"
  }
}
